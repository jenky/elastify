{
    "name": "jenky/laravel-elasticsearch",
    "type": "library",
    "description": "Elasticsearch helper for laravel",
    "keywords": [
        "jenky",
        "laravel-elasticsearch"
    ],
    "homepage": "https://github.com/jenky/laravel-elasticsearch",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynh",
            "email": "contact@lynh.me",
            "homepage": "https://github.com/jenky",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.0",
        "elasticsearch/elasticsearch": "^6.0",
        "ongr/elasticsearch-dsl": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\LaravelElasticsearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\LaravelElasticsearch\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Jenky\\LaravelElasticsearch\\ElasticsearchServiceProvider"
            ],
            "aliases": {
                "ES": "Jenky\\LaravelElasticsearch\\Facades\\ES"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
